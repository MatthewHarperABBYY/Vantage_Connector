{
  "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
  "name": "==Vantage – Attach Documents (POST multipart)",
  "id": "vantage-attach-documents",
  "version": 7,
  "description": "Attach files to an ABBYY Vantage Transaction via REST Outbound Connector.",
  "category": {
    "id": "connectors",
    "name": "Connectors"
  },
  "appliesTo": [
    "bpmn:Task"
  ],
  "elementType": {
    "value": "bpmn:ServiceTask"
  },
  "properties": [
    {
      "label": "Connector Type",
      "type": "String",
      "value": "io.camunda:rest:1",
      "binding": {
        "type": "zeebe:taskDefinition",
        "property": "type"
      }
    },
    {
      "label": "Vantage API Base URL",
      "type": "String",
      "value": "https://vantage-us.abbyy.com",
      "binding": {
        "type": "zeebe:input",
        "name": "baseUrl"
      }
    },
    {
      "label": "Transaction ID",
      "type": "String",
      "feel": "required",
      "value": "=TransactionId",
      "binding": {
        "type": "zeebe:input",
        "name": "transactionId"
      }
    },
    {
      "label": "HTTP Method",
      "type": "String",
      "value": "POST",
      "binding": {
        "type": "zeebe:input",
        "name": "method"
      }
    },
    {
      "label": "URL",
      "type": "Text",
      "feel": "required",
      "value": "=baseUrl + \"/api/publicapi/v1/transactions/\" + transactionId + \"/files\"",
      "binding": {
        "type": "zeebe:input",
        "name": "url"
      }
    },
    {
      "label": "Bearer Token (no 'Bearer ' prefix)",
      "type": "String",
      "feel": "required",
      "value": "=VantageAuth",
      "binding": {
        "type": "zeebe:input",
        "name": "token"
      }
    },
    {
      "label": "Headers",
      "type": "Text",
      "feel": "required",
      "value": "={ Authorization: \"Bearer \" + token, Accept: \"*/*\" }",
      "binding": {
        "type": "zeebe:input",
        "name": "headers"
      }
    },
    {
      "label": "Files (array of File variables)",
      "type": "String",
      "feel": "required",
      "value": "=TransactionFiles",
      "binding": {
        "type": "zeebe:input",
        "name": "filesArray"
      },
      "description": "Each item must be a File-typed variable with filename and mimeType."
    },
    {
      "label": "Registration Parameters (array of {key,value})",
      "type": "Text",
      "value": "=[]",
      "binding": {
        "type": "zeebe:input",
        "name": "registrationParameters"
      }
    },
    {
      "label": "Model payload (auto-generated)",
      "type": "Text",
      "feel": "required",
      "value": "={ files: [ for i in 0..(count(filesArray)-1) return { index: i, imageProcessingOptions: { autoCrop: \"Default\", autoOrientation: \"Default\" }, registrationParameters: registrationParameters } ] }",
      "binding": {
        "type": "zeebe:input",
        "name": "modelPayload"
      }
    },
    {
      "label": "Multipart Form Data",
      "type": "Text",
      "feel": "required",
      "value": "=flatten([[{ name: \"Model\", contentType: \"application/json\", value: modelPayload }],[ for f in filesArray return { name: \"Files\", filename: f.filename, contentType: f.mimeType, value: f } ]])",
      "binding": {
        "type": "zeebe:input",
        "name": "multipart"
      }
    },
    {
      "label": "Timeout (ms)",
      "type": "String",
      "value": "60000",
      "binding": {
        "type": "zeebe:input",
        "name": "timeout"
      }
    },
    {
      "label": "Store Raw Response",
      "type": "String",
      "value": "RawAttachDocumentsResponse",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "resultVariable"
      }
    },
    {
      "label": "Result Expression",
      "type": "Text",
      "feel": "required",
      "value": "={ status: status, body: body, headers: headers }",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "resultExpression"
      }
    },
    {
      "label": "mobileInputLink → target variable",
      "type": "String",
      "value": "TransactionMobileInputLink",
      "binding": {
        "type": "zeebe:output",
        "source": "=RawAttachDocumentsResponse.body.mobileInputLink"
      }
    }
  ],
  "groups": [
    {
      "id": "endpoint",
      "label": "HTTP Endpoint"
    },
    {
      "id": "authentication",
      "label": "Authentication"
    },
    {
      "id": "files",
      "label": "Files"
    },
    {
      "id": "advanced",
      "label": "Advanced"
    },
    {
      "id": "result",
      "label": "Result"
    }
  ],
  "icon": {
    "contents": "data:image/svg+xml;base64,"
  }
}